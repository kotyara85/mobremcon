return true;}}
return false;};ft.hasAnyBreakpointColumn=function(){for(var c in ft.columns){if(ft.columns[c].hasBreakpoint){return true;}}
return false;};ft.resize=function(){var $table=$(ft.table);if(!$table.is(':visible')){return;}
